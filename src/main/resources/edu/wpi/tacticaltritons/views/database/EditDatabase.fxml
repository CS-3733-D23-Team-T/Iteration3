<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXButton?>
<?import io.github.palexdev.materialfx.controls.MFXFilterComboBox?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane alignment="CENTER" prefHeight="650.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.tacticaltritons.controllers.database.EditDatabaseController">
    <children>
        <FlowPane fx:id="content" alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefHeight="510.0" prefWidth="350.0" style="-fx-background-color: ffffff; -fx-background-radius: 10;">
            <children>
            <FlowPane fx:id="setTablePane" alignment="CENTER" columnHalignment="CENTER" orientation="VERTICAL" prefHeight="60.0">
               <children>
                  <MFXFilterComboBox fx:id="tableCombobox" prefWidth="160.0" promptText="Select Table" />
               </children>
               <FlowPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </FlowPane.margin>
            </FlowPane>
            <MFXButton id="normalButton" fx:id="updateEntryButton" text="Update">
               <FlowPane.margin>
                  <Insets bottom="10.0" top="10.0" />
               </FlowPane.margin>
            </MFXButton>
            </children>
            <FlowPane.margin>
                <Insets right="25.0" />
            </FlowPane.margin>
        </FlowPane>
        <FlowPane fx:id="tableInsert" alignment="CENTER" columnHalignment="CENTER" prefHeight="510.0" prefWidth="800.0" style="-fx-background-color: ffffff; -fx-background-radius: 10;" />
    </children>
   <stylesheets>
      <URL value="@../../stylesheets/Database.css" />
      <URL value="@../../stylesheets/Buttons.css" />
   </stylesheets>

</FlowPane>
